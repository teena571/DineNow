PORT=5000
NODE_ENV=development

# JWT Secrets
JWT_SECRET=your_jwt_secret_here_min_32_chars
JWT_REFRESH_SECRET=your_jwt_refresh_secret_here_min_32_chars

# Google OAuth
GOOGLE_CLIENT_ID=your_google_client_id_here
GOOGLE_CLIENT_SECRET=your_google_client_secret_here

# Database
MONGO_URI=your_mongo_connection_string_here

# Payment
RAZORPAY_KEY_ID=your_razorpay_key_id_here
RAZORPAY_KEY_SECRET=your_razorpay_key_secret_here

# AI
GEMINI_API_KEY=REPLACE_WITH_YOUR_GEMINI_API_KEY

# Frontend URL (comma-separated for multiple URLs)
# Example: http://localhost:5173,https://your-app.vercel.app
FRONTEND_URL=http://localhost:5173

# Copy this file to backend/.env and fill real values. Do NOT commit the real .env file.
